    <script src="https://www.google.com/recaptcha/api.js?onload=onloadCallback&render=explicit" async defer></script>
    <script type="text/javascript">
        login.Iniciar();

        var isRecaptchaDefined = '6LfCdlQdAAAAAFtAy7goFTGyEOWQ420c791oKjba' != '' && 'True' == 'True';

        var onSubmit = function (token) {
            if (grecaptcha != undefined) {
                $("#hdfRecaptchaToken").val(token);
            }

            $("form").submit();
        };

        var onClick = function () {

            if (isRecaptchaDefined) {
                grecaptcha.execute();
            } else {
                $("form").submit();
            }
        };

        var onloadCallback = function () {

            if (isRecaptchaDefined) {
                grecaptcha.render('divRecaptcha', {
                    'sitekey': '6LfCdlQdAAAAAFtAy7goFTGyEOWQ420c791oKjba',
                    'callback': onSubmit
                });
            }
        };

        // Ao clicar no ENTER, acionar o fluxo do ReCaptcha.
        $("form").keypress(function (event) {
            if (event.keyCode == 13) {
                $("#btn-login").click();
            }
        });

        $(document).ready(function () {

            var firefox = navigator.userAgent.indexOf('Firefox') != -1;
            var chrome = navigator.userAgent.indexOf('Chrome') != -1;
            var edge = navigator.userAgent.indexOf('Edge') != -1;
            var safari = navigator.userAgent.indexOf('Safari') != -1;
            var opera = navigator.userAgent.indexOf('Opera') != -1;

            if (chrome || edge) {
                // Ocultar na versão Standalone
                if (!matchMedia('(display-mode: standalone)').matches) {
                    $("#pwaButton").show();
                } else {
                    $("#pwaButton").hide();
                }

            } else {
                $("#pwaButton").hide();
            }
        })

    </script>
